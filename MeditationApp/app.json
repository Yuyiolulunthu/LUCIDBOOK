{
  "expo": {
    "name": "LUCIDBOOK",
    "slug": "lucidbook",
    "version": "2.3.3",
    "orientation": "portrait",
    "icon": "./assets/images/lucidbook.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/lucidbook.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lucidbook",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/lucidbook.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lucidbook",
      "versionCode": 3
    },
    "web": {
      "favicon": "./assets/images/lucidbook.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "sounds": []
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "84ee6734-2720-452a-ae4a-47e575690763"
      }
    }
  }
}
