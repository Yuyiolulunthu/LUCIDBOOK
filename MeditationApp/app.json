{
  "expo": {
    "name": "LUCIDBOOK",
    "slug": "lucidbook",
    "version": "2.5.3",
    "orientation": "default",
    "icon": "./assets/images/lucidbook.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/lucidbook.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "4",
      "supportsTablet": true,
      "bundleIdentifier": "com.lucidbook",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "NSCameraUsageDescription": "LUCIDBOOK 需要使用相機以完成您的大頭貼設定",
        "NSPhotoLibraryUsageDescription": "LUCIDBOOK 需要存取您的相簿以選擇大頭貼照片"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/lucidbook.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lucidbook",
      "versionCode": 34,
      "permissions": [
        "CAMERA",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/images/lucidbook.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "sounds": []
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": false,
          "cameraPermission": "LUCIDBOOK 需要使用相機以完成您的大頭貼設定"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "84ee6734-2720-452a-ae4a-47e575690763" 
      }
    }
  }
}
